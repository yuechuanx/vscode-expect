{
	"name": "vscode-expect-outline",
	"displayName": "Expect Outline",
	"description": "show outline for expect scripts",
	"version": "0.0.2",
	"publisher": "yuechuanx",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/yuechuanx/vscode-expect.git"
	},
	"homepage": "https://github.com/yuechuanx/vscode-expect/blob/master/README.md",
	"activationEvents": [
		"onLanguage:expect"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "expect",
				"aliases": [
					"Expect",
					"expect"
				],
				"extensions": [
					".exp",
					".expect"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "expect",
				"scopeName": "source.exp",
				"path": "./syntaxes/expect.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "expect",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.45.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
